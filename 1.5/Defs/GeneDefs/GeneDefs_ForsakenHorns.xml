<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<GeneDef ParentName="GeneHeadboneBase">
		<defName>FO_HornsBull</defName>
		<label>forsaken bull horns</label>
		<description>Carriers of this gene grow two huge bull-like horns with yellow tips protruding from the forehead as well as several smaller horns on their chin and atop their head.</description>
		<iconPath>UI/Icons/Genes/FO_HornsBull</iconPath>
		<displayOrderInCategory>201</displayOrderInCategory>
		<selectionWeight>0</selectionWeight>
		<!-- <graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/FO_HornsBull/FO_HornsBull</graphicPath>
			<layer>PostHeadgear</layer>
		</graphicData> -->
    <renderNodeProperties>
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPaths>
          <li>Things/Pawn/Humanlike/HeadAttachments/FO_HornsBull/FO_HornsBull</li>
        </texPaths>
        <color>(128, 128, 128)</color>
        <parentTagDef>Head</parentTagDef>
        <drawData>
          <defaultData>
            <layer>80</layer>
          </defaultData>
          <dataNorth>
            <layer>10</layer>
          </dataNorth>
        </drawData>
      </li>
    </renderNodeProperties>
	</GeneDef>
	
